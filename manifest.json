{
  "name": "Twitch Bet Recorder",
  "description": "Record twitch betting history",
  "version": "0.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.twitch.tv/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [],
  "action": {
    "default_popup": "popup.html"
  }
}
